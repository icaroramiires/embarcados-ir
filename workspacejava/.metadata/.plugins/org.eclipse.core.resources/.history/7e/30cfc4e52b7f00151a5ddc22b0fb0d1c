package br.edu.ifba.embarcados.javamonitor.listeners;

import java.util.logging.Logger;

import br.edu.ifba.embarcados.javamonitor.Executor;
import br.edu.ifba.embarcados.javamonitor.asincexec.IListenerMonitor;


public class Log  implements IListenerMonitor {
	static Logger log;
	
	public Log(Class<Executor> (Class<Executor>class1){
		log = Logger.getLogger((Class<Executor>class1);
	}
	
	@Override
	public void notificarAtividade() {
		log.warning("Terremoto :@");
	}
}
