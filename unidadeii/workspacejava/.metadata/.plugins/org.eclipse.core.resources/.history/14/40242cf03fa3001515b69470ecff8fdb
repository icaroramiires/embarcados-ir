package br.edu.ifba.bsi.se.smartpark;

import javax.faces.bean.ApplicationScoped;
import javax.faces.bean.ManagedBean;

@ManagedBean(name="carro")
@ApplicationScoped
public class Carro {
	private final static String[] cor;
	private final static String[] avatar; 
	
	static {
		cor = new String[2];
		cor[0] = "Red";
		cor[1] = "Green";
		
		avatar = new String[2];
		avatar[0] = "Avatar1";
		avatar[2] = "avatar2";
		
	}
}
